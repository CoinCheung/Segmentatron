{
"model": 
    {
    "backbone": "SegNet",
    "in_channel": 3 ,
    "num_class": 12,
    "class_weight": [0.2595, 0.1826, 4.5640, 0.1417, 0.9051, 0.3826, 9.6446, 1.8418, 0.6823, 6.2478, 7.3614, 0]
    },
"optimizer": 
    {
    "base_lr": 0.1,
    "momentum": 0.9,
    "lr_policy": "step",
    "gamma": 1,
    "stepsize": 10000000,
    "weight_decay": 0.0005
    },
"train": 
    {
    "datasets": "CamVid",
    "max_iter": 10000,
    "batch_size": 4,
    "snapshot_iter": 2000,
    "valid_iter": 500,
    "out_path": "./train" 
    },
"test": 
    {
    "skip_test": true
    },
"val": 
    {
    "datasets": "CamVid",
    "batch_size": 32
    },
"out_pth": "./train/" 
}
